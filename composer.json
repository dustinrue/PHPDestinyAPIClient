{
    "name": "phpdestinyapiclient/phpdestinyapiclient",
    "type": "library",
    "description": "A Destiny API client written in PHP",
    "license": "MIT",
    "authors": [
        {
            "name": "Dustin Rue"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-curl": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "5.3.0"
    },
    "require-dev": {
        "ext-curl": "*",
        "psr/log": "^1.0",
        "phpunit/phpunit": "^4.0"
    },
    "autoload": {
        "psr-4": {
                "PHPDestinyAPIClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPDestinyAPIClient\\Tests\\": "tests/"
        }
    }
}
